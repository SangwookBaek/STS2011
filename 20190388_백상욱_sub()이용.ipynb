   "source": [
    "import re\n",
    "input_text= \"Please, square the following numbers, 3 7 11 13 17 19\"\n",
    "def squared(match):\n",
    "    value = int(match.group())\n",
    "    return str(value*value)\n",
    "p = re.compile(r'\\d+')\n",
    "p.sub(squared,input_text)\n",
    "#print(\"출력: Please,square the following numbers,\",output)"
